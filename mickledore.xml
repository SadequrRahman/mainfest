<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    
    <!-- Remote repositories definition -->
    <remote name="yocto" fetch="https://git.yoctoproject.org/git"/>
    <remote name="qt5" fetch="https://github.com/meta-qt5"/>
    <remote name="build" fetch="https://github.com/SadequrRahman/"/>
    <remote name="oe" fetch="https://github.com/openembedded"/>
    <remote name="stm32mp" fetch="https://github.com/STMicroelectronics"/>

    <!-- Upstream Yocto & OpenEmbedded Projects -->
    <project remote="yocto"     revision="mickledore" name="poky"                path="poky"/>
    <project remote="oe"        revision="mickledore" name="meta-openembedded"   path="meta-openembedded"/>
    <project remote="yocto"     revision="mickledore" name="meta-virtualization" path="meta-virtualization"/>
    <project remote="yocto"     revision="mickledore" name="meta-dpdk"           path="meta-dpdk"/>
    <project remote="yocto"     revision="mickledore" name="meta-intel"          path="meta-intel"/>
    <project remote="yocto"     revision="mickledore" name="meta-security"       path="meta-security"/>
    <project remote="yocto"     revision="mickledore" name="meta-selinux"        path="meta-selinux"/>
    <project remote="yocto"     revision="mickledore" name="meta-cgl"            path="meta-cgl"/>
    <project remote="yocto"     revision="mickledore" name="meta-cloud-services" path="meta-cloud-services"/>
    <project remote="qt5"       revision="mickledore" name="meta-qt5"            path="meta-qt5"/>
    <project remote="stm32mp"   revision="mickledore" name="meta-st-stm32mp"     path="meta-st-stm32mp"/>
    <project remote="stm32mp"   revision="mickledore" name="meta-st-openstlinux" path="meta-st-openstlinux"/>
    <project remote="stm32mp"   revision="mickledore" name="meta-st-stm32mp-addons" path="meta-st-stm32mp-addons"/>
    <project remote="build"     revision="main"      name="mainfest"            path="build"/>

</manifest>
